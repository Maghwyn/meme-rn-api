{
	"name": "meme-rn-api",
	"version": "1.1.0-dev.1",
	"private": true,
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"test:cov": "NODE_ENV=test nyc mocha --config=test/.mocharc.json --exit",
		"test:e2e": "NODE_ENV=test mocha --config=test/.mocharc-e2e.json --exit",
		"test:unit": "NODE_ENV=test mocha --config=test/.mocharc-unit.json --exit",
		"postinstall": "husky install && chmod +x .husky/pre-commit && chmod +x .husky/commit-msg"
	},
	"dependencies": {
		"@nestjs/common": "^10.3.3",
		"@nestjs/core": "^10.3.3",
		"@nestjs/event-emitter": "^2.0.4",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.3.3",
		"@nestjs/throttler": "^5.1.2",
		"argon2": "^0.40.1",
		"axios": "^1.6.7",
		"byte-size": "^8.1.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.4.5",
		"filenamify": "^6.0.0",
		"mongodb": "^6.4.0",
		"node-mailjet": "^6.0.5",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.2.1",
		"rimraf": "^5.0.5",
		"rxjs": "^7.8.1",
		"sharp": "^0.33.2",
		"stripe": "^14.19.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.0.3",
		"@commitlint/config-conventional": "^19.0.3",
		"@nestjs/cli": "^10.3.2",
		"@nestjs/schematics": "^10.1.1",
		"@nestjs/testing": "^10.3.3",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^9.2.6",
		"@types/byte-size": "^8.1.2",
		"@types/chai": "^4.3.12",
		"@types/cookie": "^0.6.0",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^4.17.21",
		"@types/mocha": "^10.0.6",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.11.24",
		"@types/passport-jwt": "^4.0.1",
		"@types/passport-local": "^1.0.38",
		"@types/sinon": "^17.0.3",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"chai": "^5.1.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "8.0.3",
		"lint-staged": "^15.2.2",
		"mocha": "^10.3.0",
		"mongodb-memory-server": "^9.1.6",
		"nyc": "^15.1.0",
		"prettier": "^3.2.5",
		"sinon": "^17.0.1",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.4",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "4.2.0",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=18"
	}
}
